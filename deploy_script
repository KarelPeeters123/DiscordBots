FILE=/tmp/deploy.pid  
if [ -f $FILE ]; then
   kill -15 $(cat $FILE)
   BUILD_ID=dontKillMe nohup python3 /home/karelpeeters/.jenkins/workspace/cato\ bot/bot.py &
   echo $! > /tmp/deploy.pid
else
   BUILD_ID=dontKillMe nohup python3 /home/karelpeeters/.jenkins/workspace/cato\ bot/bot.py&
   echo $! > /tmp/deploy.pid
fi
